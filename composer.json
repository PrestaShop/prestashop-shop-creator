{
  "name": "prestashop/shop-generator",
  "require": {
    "php": ">=7.0.0",
    "fakerphp/faker": "^1.13",
    "symfony/console": "^3.4|^4.4|^5.4",
    "symfony/filesystem": "^3.4|^4.4|^5.4",
    "symfony/yaml": "^3.4|^4.4|^5.4",
    "incenteev/composer-parameter-handler": "^2.1",
    "doctrine/inflector": "^1.2",
    "symfony/finder": "^3.4|^4.4|^5.4",
    "mossadal/math-parser": "^1.3",
    "ext-SimpleXML": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.2",
    "phpstan/phpstan": "^1.9",
    "friendsofphp/php-cs-fixer": "^v3.2.0"
  },
  "autoload": {
    "psr-4": {
      "ShopGenerator\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
    ]
  },
  "extra": {
    "incenteev-parameters": {
      "file": "app/config/config.yml",
      "parameter-key": "parameters"
    },
    "patches": {
      "fakerphp/faker": {
        "Make image provider works again": "./patches/faker-provider-image.patch"
      }
    },
    "composer-exit-on-patch-failure": true
  },
  "config": {
    "allow-plugins": {
      "cweagans/composer-patches": true
    }
  }
}
